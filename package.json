    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^3.2.4",
    "wrangler": "^3.60.0"
